@article{Machleidt2001,
abstract = {We present a charge-dependent one-boson-exchange nucleon-nucleon [Formula Presented] potential that fits the world proton-proton data below 350 MeV available in the year 2000 with a [Formula Presented] per datum of 1.01 for 2932 data and the corresponding neutron-proton data with [Formula Presented] for 3058 data. This reproduction of the [Formula Presented] data is more accurate than by any phase-shift analysis and any other [Formula Presented] potential. This is achieved by the introduction of two effective [Formula Presented] mesons the parameters of which are partial-wave dependent. The charge dependence of the present potential (which we call “CD-Bonn”) is based upon the predictions by the Bonn full model for charge symmetry and charge-independence breaking in all partial waves with [Formula Presented] The potential is represented in terms of the covariant Feynman amplitudes for one-boson exchange which are nonlocal. Therefore, the off-shell behavior of the CD-Bonn potential differs in a characteristic way from commonly used local potentials and leads to larger binding energies in nuclear few- and many-body systems, where underbinding is a persistent problem. {\textcopyright} 2001 The American Physical Society.},
archivePrefix = {arXiv},
arxivId = {nucl-th/0006014},
author = {Machleidt, R.},
doi = {10.1103/PhysRevC.63.024001},
eprint = {0006014},
file = {:home/yuriy/Documents/CURRENT/PHD{\_}TEMPLATE/Literature/PhysRevC.63.024001.pdf:pdf},
issn = {1089490X},
journal = {Physical Review C - Nuclear Physics},
number = {2},
pages = {32},
primaryClass = {nucl-th},
title = {{High-precision, charge-dependent {Bonn} nucleon-nucleon potential}},
volume = {63},
year = {2001}
}
